{
  "name": "search-ui",
  "version": "0.2.0-development.2",
  "private": true,
  "license": "MIT",
  "readme": "README.md",
  "dependencies": {
    "@advtr/tidy": "^0.2.0-beta.4",
    "@harrytwright/logger": "^3.0.0",
    "clsx": "^1.1.1",
    "gh-pages": "^4.0.0",
    "immer": "^9.0.15",
    "intl": "1.2.5",
    "lodash.get": "^4.4.2",
    "lodash.includes": "^4.3.0",
    "lodash.split": "^4.4.2",
    "path-browserify": "^1.0.1",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-feather": "^2.0.10",
    "react-ga": "^3.3.1",
    "react-intl": "^6.0.5",
    "react-redux": "^8.0.2",
    "redux": "^4.2.0",
    "redux-saga": "^1.2.0",
    "reselect": "^4.1.6",
    "sanitize-html": "^2.7.1",
    "sanitize.css": "^13.0.0",
    "styled-components": "5.3.5",
    "throttle-debounce": "^5.0.0",
    "ts-imgix": "^0.0.18",
    "url": "^0.11.0",
    "util": "^0.12.4",
    "web-vitals": "^2.1.4",
    "wikipedia-tldr": "^2.0.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "start:storybook": "storybook dev -p 6006 -s public",
    "prebuild:intl": "sh ./scripts/pre-extract.sh",
    "build": "react-scripts build",
    "build:intl": "node ./scripts/extract-intl.js",
    "build:storybook": "storybook build -s public",
    "build:acknowledgments": "node ./scripts/license.js",
    "postbuild:intl": "sh ./scripts/post-extract.sh",
    "deploy:gh-pages": "gh-pages",
    "deploy:chromatic": "chromatic -b build:storybook --exit-zero-on-changes",
    "lint": "eslint ./src ./.storybook ./scripts --ext .jsx --ext .js",
    "lint:fix": "eslint ./src ./.storybook ./scripts --fix --ext .jsx --ext .js",
    "test": "react-scripts test",
    "test:coverage": "react-scripts test --coverage",
    "eject": "react-scripts eject"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "eslintConfig": {
    "env": {
      "jest/globals": true,
      "browser": true,
      "es2021": true
    },
    "extends": [
      "plugin:react/recommended",
      "standard"
    ],
    "parserOptions": {
      "ecmaFeatures": {
        "jsx": true
      },
      "ecmaVersion": "latest",
      "sourceType": "module"
    },
    "plugins": [
      "jest",
      "react"
    ],
    "rules": {
      "react/jsx-uses-react": [
        "error"
      ]
    },
    "settings": {
      "react": {
        "version": "detect"
      }
    },
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  },
  "devDependencies": {
    "@babel/cli": "^7.18.10",
    "@babel/core": "^7.18.10",
    "@babel/eslint-parser": "^7.18.9",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-modules-commonjs": "^7.18.6",
    "@babel/plugin-transform-react-constant-elements": "^7.18.12",
    "@babel/plugin-transform-react-inline-elements": "^7.18.6",
    "@babel/preset-env": "^7.18.10",
    "@babel/preset-react": "^7.18.6",
    "@babel/register": "^7.18.9",
    "@storybook/addon-a11y": "^7.0.0-alpha.18",
    "@storybook/addon-actions": "^7.0.0-alpha.18",
    "@storybook/addon-essentials": "^7.0.0-alpha.18",
    "@storybook/addon-interactions": "^7.0.0-alpha.18",
    "@storybook/addon-links": "^7.0.0-alpha.18",
    "@storybook/builder-webpack5": "^7.0.0-alpha.18",
    "@storybook/cli": "^7.0.0-alpha.18",
    "@storybook/manager-webpack5": "^7.0.0-alpha.10",
    "@storybook/node-logger": "^7.0.0-alpha.18",
    "@storybook/preset-create-react-app": "^4.1.2",
    "@storybook/react": "^7.0.0-alpha.18",
    "@storybook/react-webpack5": "^7.0.0-alpha.18",
    "@storybook/testing-library": "^0.0.14-next.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "13.3.0",
    "@testing-library/user-event": "^14.4.3",
    "babel-core": "7.0.0-bridge.0",
    "babel-jest": "^28.1.3",
    "babel-loader": "^8.2.5",
    "babel-plugin-dynamic-import-node": "^2.3.3",
    "babel-plugin-lodash": "3.3.4",
    "babel-plugin-module-resolver": "^4.1.0",
    "babel-plugin-react-intl": "^8.2.25",
    "babel-plugin-styled-components": "^2.0.6",
    "babel-plugin-transform-react-remove-prop-types": "0.4.24",
    "chalk": "4.1.2",
    "chromatic": "^6.7.4",
    "eslint": "^8.22.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^26.8.3",
    "eslint-plugin-n": "^15.2.4",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-react": "^7.30.1",
    "eslint-plugin-storybook": "^0.6.4",
    "file-system-cache": "^2.0.0",
    "glob": "^8.0.3",
    "jest": "^28.1.3",
    "jest-styled-components": "^7.1.0",
    "react-scripts": "^5.0.0",
    "react-test-renderer": "^18.2.0",
    "serve": "^14.0.1",
    "shelljs": "^0.8.4",
    "storybook": "^7.0.0-alpha.13",
    "storybook-addon-mock": "^2.4.1",
    "webpack": "^5.74.0"
  },
  "jest": {
    "moduleNameMapper": {
      "^@advtr/tidy$": "<rootDir>/node_modules/@advtr/tidy/dist/cjs/index.js"
    },
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      ".*stories.js"
    ]
  }
}
